<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>JSPHttpServletProject</display-name>
  <welcome-file-list>
    <welcome-file>pageOne.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <display-name>Page 1 Process</display-name>
    <servlet-name>pageOneProcess</servlet-name>
    <servlet-class>com.as.samples.pageOneProcess</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pageOneProcess</servlet-name>
    <url-pattern>/process1</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Page 2 Process</display-name>
    <servlet-name>pageTwoProcess</servlet-name>
    <servlet-class>com.as.samples.pageTwoProcess</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pageTwoProcess</servlet-name>
    <url-pattern>/process2</url-pattern>
  </servlet-mapping>
</web-app>